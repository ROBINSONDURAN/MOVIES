<h1>LISTA DE DOCUMENTALES</h1>

<table>

            <thead>
                <tr> 
                    <th scope="col">Nombre</th>
                    <th scope="col">Director</th>
                    <th class="col-large"  scope="col">Sinopsis</th>
                </tr>
            </thead>
            <tbody>
                <% @doc_all.each do |doc| %>
                    <tr>
                        <td> <%= doc.name %> </td>
                        <td> <%= doc.director %> </td>
                        <td> <%= doc.synopsis %> </td>
                    </tr>
                <% end %>
            </tbody>
        </table>